        android:supportsRtl = "true"
        android:theme = "@style/Theme.UserKotlin">
        <activity android:name = ".Notifiction.Notifiction_Activity">
        </activity>

        <service
            android:name = ".MyService"
            android:enabled = "true"
            android:exported = "false">
        </service>

        <activity android:name = ".bascket.MainBascket">
        </activity>
        <activity android:name = ".PartMain.MainActivityPart" />
        <activity android:name = ".MainActivity" />
        <activity android:name = ".MainFood">
            <intent-filter>
                <action android:name = "android.intent.action.MAIN" />

                <category android:name = "android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>